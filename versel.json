{
    "version": 2,
    "builds": [{
            "src": "legal/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": { "distDir": "staticfiles_build" }
        }
    ],
    "routes": [{
            "src": "/static_price/(.*)",
            "dest": "/static_price/$1"
        },
        {
            "src": "/(.*)",
            "dest": "legal/wsgi.py"
        }
    ]
}